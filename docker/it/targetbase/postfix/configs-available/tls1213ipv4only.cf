inet_protocols = ipv4
tls_ssl_options = NO_RENEGOTIATION
tls_preempt_cipherlist = yes
tls_medium_cipherlist = SSLCipherSuite DHE-RSA-AES256-GCM-SHA384:DHE-RSA-CHACHA20-POLY1305:DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-SHA256:DHE-RSA-AES256-SHA:DHE-RSA-AES128-SHA256:DHE-RSA-AES128-SHA
smtpd_tls_fingerprint_digest = sha256
smtpd_tls_protocols = TLSv1.2, TLSv1.3
smtpd_tls_ciphers = medium
#smtpd_tls_dh1024_param_file = ...