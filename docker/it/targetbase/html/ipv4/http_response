		<script type="text/javascript">
			function updateTelefoon(country) {
					var telefoon = {
						nl: '050 211 0100',be: '09 218 5919',int: '+31 50 211 0100'					}
					if(country in telefoon) {
						jQuery('#cn').text(telefoon[country]);
						jQuery('.telefoonnummerlink > span').text(telefoon[country]);
						jQuery('.telefoonnummerlink').attr('href', "tel:" + telefoon[country].replace(/\s/g, ""));
					} else {
						jQuery('#cn').text(telefoon['int']);
						jQuery('.telefoonnummerlink > span').text(telefoon['int']);
						jQuery('.telefoonnummerlink').attr('href', "tel:" + telefoon['int'].replace(/\s/g, ""));
					}
				}
		</script>

