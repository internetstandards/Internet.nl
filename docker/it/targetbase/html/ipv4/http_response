<script type="text/javascript">
( function( grecaptcha, sitekey, actions ) {

	var wpcf7recaptcha = {

		execute: function( action ) {
			grecaptcha.execute(
				sitekey,
				{ action: action }
			).then( function( token ) {
				var forms = document.getElementsByTagName( 'form' );

				for ( var i = 0; i < forms.length; i++ ) {
					var fields = forms[ i ].getElementsByTagName( 'input' );

					for ( var j = 0; j < fields.length; j++ ) {
						var field = fields[ j ];

						if ( 'g-recaptcha-response' === field.getAttribute( 'name' ) ) {
							field.setAttribute( 'value', token );
							break;
						}
					}
				}
			} );
		},

		executeOnHomepage: function() {
			wpcf7recaptcha.execute( actions[ 'homepage' ] );
		},

		executeOnContactform: function() {
			wpcf7recaptcha.execute( actions[ 'contactform' ] );
		},

	};

	grecaptcha.ready(
		wpcf7recaptcha.executeOnHomepage
	);

	document.addEventListener( 'change',
		wpcf7recaptcha.executeOnContactform, false
	);

	document.addEventListener( 'wpcf7submit',
		wpcf7recaptcha.executeOnHomepage, false
	);

} )(
	grecaptcha,
	'6LdHuYMUAAAAAFuovRK5X8wXkwTToCpSRY6QMTNm',
	{"homepage":"homepage","contactform":"contactform"}
);
</script>
<button id="chatknop" onclick="toonChatvenster()">
	<div class="icoon">
		<svg class="inline"><use xlink:href="#chat"/></svg>
	</div>
	<span class="titel kort">Chat</span>
	<span class="titel lang">Kunnen we je helpen?</span>
</button>
<script type="text/javascript">
	/* Begin Chatlio-script */
	window._chatlio = window._chatlio || [];
	!function() {
		var t = document.getElementById("chatlio-widget-embed");
		if (t && window.ChatlioReact && _chatlio.init) {
			return void _chatlio.init(t, ChatlioReact);
		}
		var e = function(t) {
			return function() {
				_chatlio.push([t].concat(arguments));
			};
		};
		var i = ["configure", "identify", "track", "show", "hide", "isShown", "isOnline", "page"];
		for (var a = 0; a < i.length; a++) {
			_chatlio[i[a]] || (_chatlio[i[a]] = e(i[a]));
		}
		var n = document.createElement("script");
		n.id = "chatlio-widget-embed";
		n.src = "https://w.chatlio.com/w.chatlio-widget.js";
		n.async = !0;
		n.setAttribute("data-embed-version", "2.3");
		n.setAttribute("data-widget-id", "1f80a429-d903-4b83-65d1-0ef9c2e2da0d");
		var c = document.getElementsByTagName("script")[0];
		c.parentNode.insertBefore(n, c);
	}();
	/* Einde Chatlio-script */

	document.addEventListener('chatlio.ready', function() {
		_chatlio.configure({
			titleColor: "", // Wordt ingesteld in de CSS
			titleFontColor: "", // Wordt ingesteld in de CSS
			requireInfoGreeting: "Vul je e-mailadres en je naam in om een chat te beginnen.",
			requireInfoSubmitBtn: "Begin de chat",
			autoResponseMessage: "Stel nu je vraag aan een van onze klantenservice&shy;medewerkers.",
			agentLabel: "Vevida",
			browserSideAuthorLabel: "Ik",
			onlineMessagePlaceholder: "Hoe kunnen we je helpen?",
			noAnswerWithEmail: "Sorry, het is erg druk momenteel. Je kunt nog even blijven wachten. We nemen sowieso contact met je op.",
			requestScreenshotText: "Ik zou graag een screenshot van je browserscherm maken. Let op: dit kan even duren.",
			requestScreenshotAllowLabel: "Ok",
			requestScreenshotDeclineLabel: "Nee",
			offlineGreeting: "We zijn nu niet online. Laat een bericht achter en we komen er zo snel mogelijk op terug.",
			offlineEmailPlaceholder: "Je e-mailadres",
			offlineMessagePlaceholder: "Je bericht",
			offlineNamePlaceholder: "Je naam",
			offlineSendButton: "Verzenden",
			offlineThankYouMessage: "Bedankt voor je bericht. Je hoort gauw van ons!",
			defaultOkBtnLabel: "Ok",
			defaultSendBtnLabel: "Verzenden",
			defaultCancelBtnLabel: "Annuleren",
			defaultYesBtnLabel: "Ja",
			defaultNoBtnLabel: "Nee",
			conversationEndLabel: "Deze chatsessie is beindigd.",
			conversationRatingLabel: "Hoe zou je deze chat beoordelen?",
			conversationRatingThankYou: "Bedankt voor je beoordeling!",
			conversationEndTranscriptPlaceholder: "E-mail deze chat naar mij",
			conversationTranscriptSentThankYou: "Bedankt! Je ontvangt binnenkort een e-mail van ons.",
			timeLocale: {
				"just now": "Zojuist",
				"m ago": " min. geleden",
				"h ago": " uur geleden",
				"d ago": " dagen geleden",
				"mo ago": " maanden geleden",
				"y ago": " jaar geleden",
			},
		});

		if (window.location.hash === "#chat-open") {
			toonChatvenster();
		} else {
			toonChatknop();
		}
		if (_chatlio.isOnline()) {
			toonChatOnline();
		}
		document.addEventListener("chatlio.collapsed", chatvensterGesloten);
		document.addEventListener("chatlio.messageReceived", chatvensterGeopend);
		document.addEventListener("chatlio.online", toonChatOnline);
		document.addEventListener("chatlio.offline", toonChatOffline);
	}, false);

	var chatknop = document.getElementById("chatknop");

	function toonChatknop() {
		chatknop.classList.add("zichtbaar");
	}

	function verbergChatknop() {
		chatknop.classList.remove("zichtbaar");
	}

	function toonChatvenster() {
		_chatlio.show({
			expanded: true
		});
		chatvensterGeopend();
	}

	/** Chatvenster is geopend de knop of do
548
or een bericht van de KS */
	function chatvensterGeopend() {
		verbergChatknop();
		voegBodyClassToe();
		if (window.history.pushState && window.location.hash !== "#chat-open") {
			window.history.pushState("", "", window.location.pathname + "#chat-open");
			window.onhashchange = sluitChatNaNavigatie;
		}
	}

	/** Chatvenster is gesloten door op de knop te klikken */
	function chatvensterGesloten() {
		toonChatknop();
		verwijderBodyClass();
		if (!window.history.pushState || window.location.hash !== "#chat-open") {
			return;
		}
		if (window.onhashchange === sluitChatNaNavigatie) {
			window.onhashchange = undefined;
			window.history.go(-1);
		} else {
			window.history.replaceState("", "", window.location.pathname);
		}
	}

	/** De gebruiker navigeert terug, sluit het chatvenster */
	function sluitChatNaNavigatie() {
		_chatlio.hide();
		toonChatknop();
		verwijderBodyClass();
	}

	function voegBodyClassToe() {
		document.getElementsByTagName("body")[0].classList.add("chat-open");
	}

	function verwijderBodyClass() {
		document.getElementsByTagName("body")[0].classList.remove("chat-open");
	}

	function toonChatOnline() {
		chatknop.getElementsByClassName("icoon")[0].classList.add("online");
	}

	function toonChatOffline() {
		chatknop.getElementsByClassName("icoon")[0].classList.remove("online");
	}
</script>
</body>
